import("Syntax.ShellStyle");
import("Type.Json");

boolean RoutingDefault(Json ctx) {
	boolean ret = false;

	String Rtable = $(netstat -rn | grep "UG");
	if (Rtable.search("UG") > -1) {
		System.p("Success:The gateway is registered. Check its routing settings.");
		ret = true;
	}

	return ret;
}

void main() {
	Json ctx = new Json();
	System.p(RoutingDefault(ctx));
}

main();

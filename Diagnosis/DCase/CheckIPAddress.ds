import("Syntax.ShellStyle");
import("Type.Json");

boolean CheckIPAddress(Json ctx) {
	String ifconfig = $(ifconfig);
	ifconfig = ifconfig.substring(0, ifconfig.search("lo"));
	boolean ret = (ifconfig.search("inet") > -1);
	return ret;
}

void main() {
	Json ctx = new Json();
	System.p(CheckIPAddress(ctx));
}

main();

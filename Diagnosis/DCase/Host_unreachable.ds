import("dshell");
import("konoha.json");

const DCaseContext = Json;

String IP = "";

boolean host_unreachable(DCaseContext ctx) {
	boolean ret = true;
	IP = ctx.getString("givenIP");
	String checkIP = $(minikonoha IPCheck.ds "${IP}");
	System.p(checkIP);
	return ret;
}

void main() {
	DCaseContext ctx = new DCaseContext();
	ctx.setString("givenIP", "192.168.1.1");
	host_unreachable(ctx);
}

main();
